{
  "name": "@kontext/sdk",
  "version": "0.1.0",
  "description": "Trust and compliance layer for agentic crypto workflows",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "kontext",
    "compliance",
    "crypto",
    "usdc",
    "stablecoin",
    "agent",
    "trust",
    "audit",
    "genius-act"
  ],
  "author": "Kontext Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kontext-hq/kontext-sdk"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
